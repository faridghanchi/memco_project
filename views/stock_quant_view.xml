<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        
        <record id="view_stock_quant_form_view_memco" model="ir.ui.view">
            <field name="name">stock.quant.form.view.memco</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_form"/>
            <field name="arch" type="xml">
                <data>
                <field name="inventory_value" position="after">
                    <field name="local_carrier_cost" />
                    <field name="international_c_cost" />
                    <field name="unit_lc_cost" />
                    <field name="total_cost" />
                </field>
                </data>
            </field>
        </record>
        
        <record id="view_stock_quant_tree_view_memco" model="ir.ui.view">
            <field name="name">stock.quant.tree.view.memco</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
            <field name="arch" type="xml">
                <data>
                <field name="inventory_value" position="after">
                    <field name="total_cost" />
                </field>
                </data>
            </field>
        </record>
        
    </data>
</openerp>
